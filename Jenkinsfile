pipeline {
  agent any
  stages {
    stage('sravan kumar') {
      parallel {
        stage('sravan kumar') {
          steps {
            sleep(unit: 'MINUTES', time: 1)
          }
        }

        stage('') {
          steps {
            sh 'echo "My name is sravan"'
          }
        }

      }
    }

  }
}